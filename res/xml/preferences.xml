<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
  
  <CheckBoxPreference
    android:defaultValue="true"
    android:key="wifi_only"
    android:title="Sync over Wi-Fi Only" />
  
  <PreferenceScreen
        android:title="Add iiNet Account">
        <intent
             android:targetPackage="au.id.teda.broadband.usage"
             android:targetClass="au.id.teda.broadband.usage.authenticator.AuthenticatorActivity" />         
    </PreferenceScreen>
    
  <PreferenceScreen
        android:title="Chart Temp">
        <intent
             android:targetPackage="au.id.teda.broadband.usage"
             android:targetClass="au.id.teda.broadband.usage.ui.ChartActivity" />         
    </PreferenceScreen>

</PreferenceScreen>